   <children>
      <Pane prefHeight="279.0" prefWidth="241.0">
         <children>
            <Label layoutX="14.0" layoutY="63.0" text="UserName" />
            <Label layoutX="14.0" layoutY="107.0" text="PassWord" />
            <TextField fx:id="userName" layoutX="79.0" layoutY="63.0" />
            <Button layoutX="183.0" layoutY="147.0" mnemonicParsing="false" onAction="#logInButton" text="Login" />
            <Button layoutX="79.0" layoutY="214.0" mnemonicParsing="false" onAction="#canceLButton" text="Cancel" />
            <Button layoutX="175.0" layoutY="214.0" mnemonicParsing="false" onAction="#signUpButton" text="SignUp" />
